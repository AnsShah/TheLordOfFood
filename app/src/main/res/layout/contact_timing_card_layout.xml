<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@drawable/product_border_shape_card"
    android:layout_margin="10dp"
    app:ignore="NamespaceTypo"
    android:visibility="gone">
    <RelativeLayout
        android:layout_margin="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_centerHorizontal="true"
            android:layout_margin="10dp"
            android:id="@+id/idContactTimingLinearLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/margin_10dp"
            android:layout_marginLeft="@dimen/margin_10dp"
            android:orientation="vertical">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:background="@drawable/logo2"
                android:tint="@color/backImageColor"></ImageView>
            <TextView
                android:fontFamily="@font/open_sans_regular"
                android:textColor="@color/headingTitleColor"
                android:textSize="18dp"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lord Of Food"
                android:layout_gravity="center"
                android:layout_marginBottom="10dp"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_below="@+id/idContactTimingLinearLayout"
            android:id="@+id/line2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#c0c0c0"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#c0c0c0"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/idContactLinearLayout"
            android:background="@drawable/product_border_shape_card_layout1"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/line2"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">
            <TextView
                android:id="@+id/idTextContactLinearLayout"
                android:fontFamily="@font/open_sans_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact"
                android:layout_margin="10dp"
                android:textSize="20dp"
                android:textColor="@color/headingTitleColor"></TextView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/idTimingLinearLayout"
            android:background="@drawable/product_border_shape_card_layout1"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/idContactLinearLayout"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">
            <TextView
                android:id="@+id/idTextTimingLinearLayout"
                android:fontFamily="@font/open_sans_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Timings"
                android:layout_margin="10dp"
                android:textSize="20dp"
                android:textColor="@color/headingTitleColor"></TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_margin="10dp"
            android:id="@+id/idContactInfoLinearLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_below="@+id/idTimingLinearLayout">
            <TextView
                android:fontFamily="@font/open_sans_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact Info"
                android:textColor="@color/headingTitleColor"
                android:textSize="20dp"></TextView>
            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:padding="3dp"
                        android:background="@drawable/grey_item_back"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_baseline_location_on_24"
                        app:tint="@color/backImageColor"></ImageView>
                </RelativeLayout>
                <TextView
                    android:layout_marginLeft="5dp"
                    android:fontFamily="@font/open_sans_regular"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/headingTitleColor"
                    android:textStyle="bold"
                    android:text="@string/contact_us"></TextView>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/idPhoneLinearLayout"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:padding="3dp"
                        android:background="@drawable/grey_item_back"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_baseline_phone_24"
                        app:tint="@color/backImageColor"></ImageView>
                </RelativeLayout>
                <TextView
                    android:id="@+id/idTextPhone"
                    android:layout_marginLeft="5dp"
                    android:fontFamily="@font/open_sans_regular"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/headingTitleColor"
                    android:textStyle="bold"
                    android:text="+923094888111"></TextView>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/idTimingTableLinearLayout"
android:visibility="gone"
            android:layout_margin="10dp"
            android:background="@drawable/product_border_shape_card"
            android:layout_below="@+id/idContactInfoLinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_centerHorizontal="true"
            android:gravity="center">
            <TableLayout
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                    android:background="@drawable/table_border_shape_layout">
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:background="@drawable/table_border_shape_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Monday"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"></TextView>
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 12:00PM - 12:00AM "
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"
                        ></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/table_border_shape_layout">
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:background="@drawable/table_border_shape_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Tuesday"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"></TextView>
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 12:00PM - 12:00AM "
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"
                        ></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/table_border_shape_layout">
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:background="@drawable/table_border_shape_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Wednesday"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"></TextView>
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 12:00PM - 12:00AM "
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"
                        ></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/table_border_shape_layout">
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:background="@drawable/table_border_shape_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Thursday"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"></TextView>
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 12:00PM - 12:00AM "
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"
                        ></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/table_border_shape_layout">
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:background="@drawable/table_border_shape_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Friday"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"></TextView>
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 12:00PM - 12:00AM "
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"
                        ></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/table_border_shape_layout">
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:background="@drawable/table_border_shape_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Saturday"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"></TextView>
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 12:00PM - 12:00AM "
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"
                        ></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/table_border_shape_layout">
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:background="@drawable/table_border_shape_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sunday"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"></TextView>
                    <TextView
                        android:fontFamily="@font/open_sans_regular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 12:00PM - 12:00AM "
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/headingTitleColor"
                        ></TextView>
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
